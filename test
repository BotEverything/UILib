    UIListLayout.Padding = UDim.new(0.0299999993, 0)

    local function RJVWD_fake_script() -- topbar.exit 
        local script = Instance.new('LocalScript', topbar)
    
        local btn = script.Parent.exit
        local gui = script.Parent.Parent.Parent
        
        if gui.Name == "ByteHub" then
            print("GUI has been found")
        end
        
        local function close()
            btn.MouseButton1Click:Connect(function()
                gui:Destroy()
            end)
        end
        
        close()
    end

function library:NewTab(name)
    local tab1 = Instance.new("Frame")
    local TextButton = Instance.new("TextButton")
    local Frame = Instance.new("Frame")
    
    tab1.Name = name.. "Tab"
    tab1.Parent = tabs_2
    tab1.AnchorPoint = Vector2.new(0.5, 0.5)
    tab1.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    tab1.BackgroundTransparency = 1.000
    tab1.BorderColor3 = Color3.fromRGB(0, 0, 0)
    tab1.BorderSizePixel = 0
    tab1.Position = UDim2.new(0.500000119, 0, 0.0398215726, 0)
    tab1.Size = UDim2.new(0.876633704, 0, 0.0796431452, 0)
    
    TextButton.Parent = tab1
    TextButton.BackgroundColor3 = Color3.fromRGB(55, 55, 55)
    TextButton.BackgroundTransparency = 1.000
    TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
    TextButton.BorderSizePixel = 0
    TextButton.Position = UDim2.new(0.0362182111, 0, 0, 0)
    TextButton.Size = UDim2.new(0.91818893, 0, 0.965000689, 0)
    TextButton.Font = Enum.Font.GothamMedium
    TextButton.Text = name
    TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    TextButton.TextSize = 14.000
    TextButton.TextXAlignment = Enum.TextXAlignment.Left
    
    Frame.Parent = tab1
    Frame.AnchorPoint = Vector2.new(0.5, 0.5)
    Frame.BackgroundColor3 = Color3.fromRGB(85, 85, 255)
    Frame.BackgroundTransparency = 1.000
    Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
    Frame.BorderSizePixel = 0
    Frame.Position = UDim2.new(1.00387597, 0, 0.49000001, 0)
    Frame.Size = UDim2.new(0.0149999997, 0, 1, 0)
